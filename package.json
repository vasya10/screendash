{
  "name": "screendash",
  "version": "1.0.0",
  "description": "A Handsome based ReactJS Dashboard Framework ",
  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "postinstall": "yarn run build",
    "build": "webpack --progress --colors",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --require ignore-styles --require test/setup.js --recursive",
    "lint": "eslint .",
    "cover": "./node_modules/.bin/nyc --reporter=lcov --extension .jsx npm run test",
    "cover:report": "npm run cover && ./node_modules/.bin/nyc report",
    "coveralls": "npm run cover:report && cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davefp/handsome.git"
  },
  "keywords": [
    "dashboard"
  ],
  "author": "Vasu Srinivasan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasya10/screendash/issues"
  },
  "homepage": "https://github.com/vasya10/screendash",
  "dependencies": {
    "animate.css": "^3.6.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.24.1",
    "cacheman": "^2.2.1",
    "cacheman-redis": "^1.1.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "dockerode": "^2.5.3",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "file-loader": "^1.1.5",
    "jquery": "^2.2.3",
    "lodash": "^4.17.4",
    "moment": "^2.12.0",
    "node-sass": "^4.7.2",
    "numeral": "^1.5.3",
    "packery": "^2.0.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-grid-layout": "^0.16.0",
    "redis": "^2.6.0-1",
    "request": "^2.71.0",
    "request-promise-native": "^1.0.5",
    "require-all": "^2.0.0",
    "rss-parser": "^2.10.8",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "sync-request": "^4.1.0",
    "twitter": "^1.7.1",
    "twitter-text": "^1.14.7",
    "url-loader": "^0.6.2",
    "victory": "^0.25.6",
    "webpack": "^3.10.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.4.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "enzyme": "^2.3.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "ignore-styles": "^2.0.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4",
    "react-addons-test-utils": "^15.1.0",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack-dev-middleware": "^2.0.6"
  }
}
